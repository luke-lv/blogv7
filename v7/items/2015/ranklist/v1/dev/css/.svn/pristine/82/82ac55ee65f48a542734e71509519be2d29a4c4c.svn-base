/*css reset*/
body{font:100%/1 "Helvetica Neue",Helvetica,STHeiTi,"Microsoft YaHei";}
body,h1,h2,h3,h4,h5,h6,dl,dt,dd,ul,ol,li,th,td,p,blockquote,pre,form,fieldset,legend,input,button,textarea,article,aside,footer,header,hgroup,nav,section,menu{margin:0; padding:0;-webkit-text-size-adjust:100%;}
table{border-collapse:collapse;border-spacing:0;}
address,caption,cite,code,dfn,em,strong,th,var,i{font-style:normal;font-weight:normal;}
ol,ul{list-style:none;}
abbr,acronym,fieldset,img{border:0 none;}
input,img{vertical-align:middle;}
article,aside,footer,header,hgroup,nav,section,menu,small{display:block;}
caption,th {text-align:left;}
h1,h2,h3,h4,h5,h6 {font-size:100%;font-weight:normal;}
q:before,q:after {content:'';}
a {text-decoration:none; outline:none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
a:hover {text-decoration: none;}
img { -ms-interpolation-mode:bicubic; image-rendering:optimizeQuality; }
input,
textarea {outline:none; font:12px "Microsoft Yahei","微软雅黑","STHeiti Light","华文细黑","SimSun","宋体",Arial,sans-serif;}
input[type="text"],
input[type="search"],
input[type="password"] { -webkit-border-radius:0; -moz-border-radius:0; -o-border-radius:0; border-radius:0; -webkit-appearance: none;  -webkit-tap-highlight-color: rgba(0,0,0,0); -webkit-user-modify: read-write-plaintext-only;}
textarea { resize:none }
input:-webkit-autofill {-webkit-box-shadow: 0 0 0px 1000px white inset;}