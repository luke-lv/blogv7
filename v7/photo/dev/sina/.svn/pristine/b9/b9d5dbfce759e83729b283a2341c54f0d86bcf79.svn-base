$import("sina/core/date/_date.js");
/**
 *
 *@desc the year passed is leap or not;
 *
 *@exmaple
    var mydate = new Date();
    alert(isLeap(mydate.getFullYear()));
 *
 *@author shaomin|shaomin@staff.sina.com.cn;
 */
Core.Date.isLeap = function(y){
    return y % 400 ? (y % 100 ? (y % 4 ? false : true) : false) : true;
};

  
