.quotations .flwarp .more{width:680px;margin:0 auto;padding:8px 40px 10px 0;border-top:1px solid #E1E1E1; clear:both; background:url(../../images/common/quotations/bg_arrow.gif) no-repeat center -50px; text-align:center;line-height:20px;}
