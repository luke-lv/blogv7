.qtt_feed li .qtt_feed_c .qtt_feed_txt .feed_preview{float:right;width:594px;margin:0 0 15px;}
.qtt_assign .qtt_assign_c .feed_preview{width:594px;margin:0 0 10px;}
.qtt_assign .qtt_assign_c .source{padding:0 0 6px;}
.qtt_assign .qtt_assign_c .feed_img,.qtt_feed li .qtt_feed_c .qtt_feed_txt .feed_img{float:left;margin:0 8px 10px 0;position:relative;}
.qtt_assign .qtt_assign_c .feed_img img,.qtt_feed li .qtt_feed_c .qtt_feed_txt .feed_img img{margin-right:10px;}

.quotations .frwarp{float:left;width:140px;height:800px;margin-left:-1px;background:url(../../images/common/quotations/bg_frwarp.jpg) no-repeat right 0;}
.quotations .frwarp ul li{margin-bottom:10px;}
.quotations .frwarp .diary_btn{width:135px; height:72px;background:url(../../images/common/quotations/bg_btndiary.jpg) no-repeat;}
