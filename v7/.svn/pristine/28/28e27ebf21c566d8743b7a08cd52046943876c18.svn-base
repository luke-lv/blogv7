.quotation_tb table .SG_aBtn{color:#333}

/* 日历CSS */
.pc_caldr {
  background-color: #FFFFFF;
  border: 1px solid #999999;
  height: auto;
  padding: 2px;
  width: 161px;
  z-index: 1;
}
.pc_caldr .selector {
  height: 24px;
  padding: 2px 0 0;
}
.pc_caldr .selector .month {
  border: 1px solid #CCCCCC;
  float: left;
  font-size: 12px;
  width: 73px;
}
.pc_caldr .selector .month, .pc_caldr .selector .month option {
  height: 19px;
}
.pc_caldr .selector .year {
  float: right;
  padding-left: 5px;
  width: 80px;
}
.pc_caldr .selector .year .yearval {
  float: left;
  font-family: Tahoma,sans-serif;
  font-size: 11px;
  height: 15px;
  line-height: 14px;
  margin-right: 2px;
  padding: 2px 0 0;
  text-align: center;
  width: 55px;
}
.pc_caldr .selector .year .yearbtn, .pc_caldr .selector .year .yearbtn2 {
  background: url("../../f") no-repeat scroll center center #CCCCCC;
  border: 0 none;
  display: block;
  float: left;
  margin-left: 2px;
  overflow: hidden;
  padding: 0;
  text-indent: -1000em;
}
.pc_caldr .selector .year .yearbtn {
  background: url("http://simg.sinajs.cn/blog7style/images/common/quotations/up.png") no-repeat scroll 0 0 transparent;
  height: 9px;
  width: 16px;
}
.pc_caldr .selector .year .yearbtn2 {
  background: url("http://simg.sinajs.cn/blog7style/images/common/quotations/down.png") no-repeat scroll 0 0 transparent;
  height: 9px;
  width: 16px;
}
.pc_caldr .weeks, .pc_caldr .days {
  list-style: none outside none;
  margin: 0;
  padding: 0;
  width: 100% !important;
}
.pc_caldr .weeks {
  height: 16px;
  margin-bottom: 2px;
}
.pc_caldr .days {
  height: auto;
}
.pc_caldr .weeks li, .pc_caldr .days li {
  float: left;
  height: 16px;
  line-height: 16px;
  text-align: center;
  width: 23px;
}
.pc_caldr .weeks li {
  background-color: #B6D1F9;
  color: #FFFFFF;
  font-family: "宋体",sans-serif;
  font-size: 12px;
  text-align: center;
}
.pc_caldr .days li {
  
}
.pc_caldr .days li, .pc_caldr .days li a {
  color: #000000;
  font-family: Tahoma,sans-serif;
  font-size: 11px;
  text-align: center;
}
.pc_caldr .days li a {
  color: #43609C;
  display: block;
  height: 100%;
  text-decoration: none;
}
.pc_caldr .days li a:link, .pc_caldr .days li a:visited, .pc_caldr .days li a:hover {
  text-decoration: none;
}
.pc_caldr .days li a strong {
  font-weight: normal;
}
.pc_caldr .days li.current, .pc_caldr .days li.current a, .pc_caldr .days li.current a:hover {
  background-color: #0A6A16;
  color: #FFFFFF;
}
.pc_caldr .days li a:hover {
  /*background-color: #5D94E6;
  color: #FFFFFF;*/
  background:url("http://simg.sinajs.cn/blog7style/images/common/quotations/cal_bg.gif") no-repeat scroll 0 0 transparent;
  color: #FFFFFF;
}
.pc_caldr .days .onsel {
  background-color: #5D94E6;
  color: #FFFFFF;
}
.pc_caldr .days li a.day {
  background-color: #5D94E6;
  color: #FFFFFF;
}
.weekHover {
  background-color: #5D94E6;
  color: #FFFFFF;
}
