.myWidth_L .editor_content { width:730px;}
.myWidth_L .textarea { display:block; width: 722px; }
.myWidth_L .iframe { width: 722px;}

.miniEd_Btn{ text-align:center;}
.miniEd_Btn .SG_aBtn{ margin:0px 5px;}

</style>

