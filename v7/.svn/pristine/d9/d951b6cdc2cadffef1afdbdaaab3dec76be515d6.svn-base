.mini_tools .ico_fontsize_3,
.mini_tools .ico_img_3{ background-image:url(http://sjs.sinajs.cn/blog7style/images/common/editor/mini_editor.gif); background-repeat:no-repeat; overflow:hidden; height:22px;}

.mini_tools .ico_fontsize_1 div,
.mini_tools .ico_fontsize_2 div,
.mini_tools .ico_fontsize_3 div{ margin-left: 5px; margin-top: 3px;} 

.mini_tools .ico_bold_1{background-position: -8px -71px; margin-left: 4px; margin-top: 3px; width:22px;}
.mini_tools .ico_forecolor_1{background-position: -45px -71px; margin-left: 3px; margin-top: 3px; width: 22px;}
.mini_tools .ico_fontsize_1{background-position: -83px -71px; margin-left: 10px; margin-top: 4px; width: 61px; height:20px;}
.mini_tools .ico_img_1{background-position: -156px -71px; margin-left: 7px; margin-top: 3px; width: 71px; }

.mini_tools .ico_bold_2{background-position: -8px -10px; margin-left: 4px; margin-top: 3px; width:22px;}
.mini_tools .ico_forecolor_2{background-position: -45px -10px; margin-left: 3px; margin-top: 3px; width: 22px;}
.mini_tools .ico_fontsize_2{background-position: -83px -10px; margin-left: 10px; margin-top: 4px; width: 61px; height:20px;}
.mini_tools .ico_img_2{background-position: -156px -10px;margin-left: 7px; margin-top: 3px; width: 71px; }

.mini_tools .ico_bold_3{background-position: -8px -40px; margin-left: 4px; margin-top: 3px; width:22px;}
.mini_tools .ico_forecolor_3{background-position: -45px -40px; margin-left: 3px; margin-top: 3px; width: 22px;}
.mini_tools .ico_fontsize_3{background-position: -83px -40px; margin-left: 10px; margin-top: 4px; width: 61px; height:20px;}
.mini_tools .ico_img_3{background-position: -156px -40px;margin-left: 7px; margin-top: 3px; width: 71px; }




.myWidth_S { width:210px;}
.myWidth_S .editor_title input { width:196px;}
.myWidth_S .editor_content { width:210px;}
.myWidth_S .textarea { display:block; width: 202px; }
.myWidth_S .iframe { width: 202px;}



.myWidth_M { width:510px;}
