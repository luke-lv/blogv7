/*
		模块命名规则，单位命名规则：
			固定宽度框架
			 |--.SG_wrap730
			 |--.SG_wrap510
			 |--.SG_wrap210
			  	   |--SG_colFirst       为横排第一个子框架额外class
			  	   |--.SG_colW210         等一系列子框架
					 			|---.SG_conn    模块中的内容区域
			        |--.SG_colW510
								|---.SG_conn    模块中的内容区域
*/
/* 绝对布局主框 */
.SG_wrap950{ width:950px;} /* 固定宽度框架 */
.SG_page_c .SG_wrap950{ margin:0 auto;}
/* 子框架定义 */
.SG_colW1{ width:10px;}
.SG_colW21{ width:210px;}
.SG_colW51{ width:510px;}
.SG_colW73{ width:730px;}
.SG_colW95{ width:950px;}
/* 默认宽度定义结束，如果添加宽度定义需要在本行上下各添加一项  */
.SG_colW21,
.SG_colW73{display:inline; float:left;}
/* 子框架定义 */
.SG_colW21{ width:210px;margin-left:10px;*overflow:hidden;}
.SG_colW73{ width:730px;}

/* w730 */
.SG_colW73 .SG_connHead{border-bottom:1px solid #d0d0d0; height:27px; overflow:hidden;}
.SG_colW73 .SG_connHead .title{padding-left:10px; border-top:1px solid #fff;  line-height:26px; height: 26px;display:block; font-weight:bold;border-left:1px solid #fff; background:#ededed;}
.SG_colW73 .SG_connHead .title em{font-weight:normal; font-family:宋体}
.SG_colW73 .SG_connBody{background-image: url(../../../images/common/modelbody.png);  background-position: 1450px 0;}
.SG_colW73 .SG_connFoot{background-image: url(../../../images/common/modelfoot.png);  background-position: 1450px 0; height: 1px; overflow:hidden}
/* w210 */
.SG_colW21 .SG_connHead{border-bottom:1px solid #d0d0d0; overflow:hidden; position:relative;}
.SG_colW21 .SG_connHead .title{padding-left:10px; border-top:1px solid #fff;border-right:1px solid #fff;line-height:26px; height: 26px;display:block; font-weight:bold;border-left:1px solid #fff; background:#ededed;}
.SG_colW21 .SG_connHead em{position:absolute;right:10px; top:8px;}

.SG_colW21 .SG_connBody{background:#f4f4f4; border:1px solid #fff; border-bottom:1px solid #d2d2d2; border-left:0;padding:0; _zoom:1;}
.SG_connHead .edit { float: right;padding-right: 10px;padding-top: 8px;}
